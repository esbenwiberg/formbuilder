{
  "name": "@wiberg/fluentui-builder",
  "version": "0.0.1",
  "description": "A builder of forms! It is highly customizable and can be tailored to your exact needs. As crazy as they may be..",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --incremental",
    "commit": "git add -A && cz",
    "release": "standard-version && git push --follow-tags origin main",
    "barrels": "barrelsby --delete --c ./barrelsbyconfig.json",
    "clean": "npx rimraf dist",
    "prebuildpackage": "npm run clean && npm run build && node dist/SetupPackage.js",
    "buildpackage": "cd dist && npm pack",
    "publishpackage": "cd dist && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/esbenwiberg/formbuilder"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "cz-config.js"
    }
  },
  "keywords": [
    "formbuilder",
    "formsbuilder",
    "formgenerator",
    "builder",
    "forms",
    "builder",
    "react",
    "fluentui",
    "materialui",
    "headless"
  ],
  "files": [
    "dist"
  ],
  "author": "Esben Wiberg",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.16.5",
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@fluentui/react": "^8.47.2",
    "@mdx-js/rollup": "^2.0.0-rc.1",
    "@types/node": "^16.11.12",
    "@types/react": "^17.0.37",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "barrelsby": "^2.3.1",
    "commitizen": "^4.2.4",
    "commitlint-config-gitmoji": "^2.2.5",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "^6.3.0",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": "^7.0.4",
    "prettier": "^2.5.1",
    "rollup": "^2.61.1",
    "rollup-plugin-css-bundle": "^1.0.4",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.31.1",
    "standard-version": "^9.3.2",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "@fluentui/react": "^8.47.2",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "@wiberg/formbuilder": "~0.2.2"
  }
}
