{
  "name": "@wiberg/fluentui-builder",
  "version": "0.5.10",
  "description": "A builder for the '@wiberg/formbuilder' using FluentUI components.",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "heft build --clean",
    "build:watch": "heft build --clean --watch",
    "barrels": "barrelsby --delete --c ./barrelsbyconfig.json",
    "clean:dist": "npx rimraf dist",
    "clean:publish": "npx rimraf publish",
    "prebuildpackages": "npm run build && node setupPackage.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/esbenwiberg/formbuilder"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "cz-config.js"
    }
  },
  "keywords": [
    "formbuilder",
    "formsbuilder",
    "formgenerator",
    "builder",
    "forms",
    "builder",
    "react",
    "fluentui",
    "materialui",
    "headless"
  ],
  "files": [
    "dist"
  ],
  "author": "Esben Wiberg",
  "license": "MIT",
  "devDependencies": {
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@fluentui/react": "^8.47.2",
    "@types/node": "^16.11.12",
    "barrelsby": "^2.3.1",
    "typescript": "^4.6.4",
    "@rushstack/heft": "^0.45.2"
  },
  "dependencies": {
    "@fluentui/react": "^8.47.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "@wiberg/formbuilder": "~0.11.7"
  }
}
