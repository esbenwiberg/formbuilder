{
  "name": "@wiberg/fluentui-builder",
  "version": "0.0.1",
  "description": "A builder of forms! It is highly customizable and can be tailored to your exact needs. As crazy as they may be..",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --incremental",
    "commit": "git add -A && cz",
    "release": "standard-version && git push --follow-tags origin main",
    "barrels": "barrelsby --delete --c ./barrelsbyconfig.json",
    "clean": "npx rimraf dist",
    "prebuildpackage": "npm run clean && npm run build && node dist/SetupPackage.js",
    "buildpackage": "cd dist && npm pack",
    "publishpackage": "cd dist && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/esbenwiberg/formbuilder"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "cz-config.js"
    }
  },
  "keywords": [
    "formbuilder",
    "formsbuilder",
    "formgenerator",
    "builder",
    "forms",
    "builder",
    "react",
    "fluentui",
    "materialui",
    "headless"
  ],
  "files": [
    "dist"
  ],
  "author": "Esben Wiberg",
  "license": "MIT",
  "devDependencies": {
    "@fluentui/react": "^8.47.2",
    "@types/node": "^16.11.12",
    "@types/react": "^17.0.37",
    "barrelsby": "^2.3.1",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "@fluentui/react": "^8.47.2",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "@wiberg/formbuilder": "~0.2.2"
  }
}
